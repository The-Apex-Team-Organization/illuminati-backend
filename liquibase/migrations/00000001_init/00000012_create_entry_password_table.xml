<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.15.xsd">

    <changeSet id="00000009_create_entry_password_table" author="Havryliuk Pavlo-Maksymyliano">

        <createTable tableName="entry_password">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false" primaryKeyName="pk_entry_password_id"/>
            </column>

            <column name="entry_password" type="TEXT">
                <constraints nullable="false"/>
            </column>

            <column name="last_updated" type="TIMESTAMP">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <rollback>
            <dropTable tableName="entry_password"/>
        </rollback>
    </changeSet>

</databaseChangeLog>
